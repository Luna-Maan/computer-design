sub <y> 2 output1
sub <y> 3 output1
sub <y> 4 output1
sub <y> 5 output1
sub <y> 5 y
label loop2

jumpne <richting> 3 loop3
sub <x> 1 output
copy <y> output1
sub <x> 2 output
copy <y> output1
sub <x> 3 output
copy <y> output1
sub <x> 4 output
copy <y> output1
sub <x> 5 output
copy <y> output1
sub <x> 5 x
label loop3

copyptr <pointer> temp
add <pointer> 1 pointer

jumpne <temp> 0 loop4
add <richting> 1 richting
jump end4
label loop4
sub <richting> 1 richting
label end4
and <richting> 3 richting

jumple <pointer> <limit> main
exit